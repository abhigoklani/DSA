#include <stack>
#include <iostream>
#include <vector>
#include <stack>
#include <iostream>
#include <vector>
#include <unordered_map>
#include <algorithm>
using namespace std;

int main() {
    vector<int> house = {1,2,4,3,5,6,7};
    int count =0,x=0;
    for(int i=1;i<=house.size();i++){
        if(i==pow(2,x)) {
            count = count+1;
            x++;
        }
    }
    cout<<count<<endl;
    
}
